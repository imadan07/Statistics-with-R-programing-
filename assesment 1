print("10 random numbers between -50 and +50 :")
RandomNum <- as.integer(runif(10, -50, +50))
print(RandomNum)
3.
fibo<- function(n) {
  if(n<= 1) {
    return(n)
  } else {
    return(fibo(n-1) + fibo(n-2))
  }
}
n = as.integer(10)
print("Fibonacci sequence:")
for(i in 0:(n-1)) {
  print(fibo(i))
  4.
