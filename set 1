data_frame<-data.frame(training=c("strength","stamina","other"),pulse=c(100,150,120),duration=c(60,30,45))
print(number)
print(thislist)
print(thismatrix)
print(data_frame)
print(direction)
print(multiarray)
10)
a<-c(1,2,3)
b<-c(4,5,6)
c<-c(7,8,9)
dm<-matrix(rbind(a,b,c),nrow=3,ncol=3)
dm
dm[2,]
dm[3,2]
dm[2,2]
dm[,3]
dm[1,]
output:-
[,1] [,2] [,3]
[1,]    1    2    3
[2,]    4    5    6
[3,]    7    8    9
> dm[2,]
[1] 4 5 6
> dm[3,2]
[1] 8
> dm[2,2]
[1] 5
> dm[,3]
[1] 3 6 9
> dm[1,]
[1] 1 2 3


